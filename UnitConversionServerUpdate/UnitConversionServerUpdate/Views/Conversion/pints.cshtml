@model int

@{
    ViewBag.Title = "pints";
}

<h2> pints life</h2>
<div id="app">
    <header id="header">
        CUPS TO PINTS
    </header>
    <hr id="top-line" />
    <div class="row">
        <div class="offset-md-1 col-md-2">
            <input id="cupsInput" v-model="cups" v-on:input="conCtoO" type="number" />
            <p class="cups">CUPS</p>
        </div>
        <div class="col-md-4">
            <p id="equal">=</p>
        </div>
        <div class="col-md-2">
            <input id="secondInput" v-model="oz" v-on:input="conOtoC" type="number">
            <p class="cups"> PINTS</p>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">

        </div>
        <div class="col-md-5">

        </div>
    </div>
    <hr id="bottom-line" />
</div> 







